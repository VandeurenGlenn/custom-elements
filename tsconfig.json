{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "nodenext",
    "moduleDetection": "auto",
    "esModuleInterop": true,
    "outDir": "exports",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["html", "rollup-plugin-material-symbols"]
  },
  "include": ["./**/*", ".vscode/elements.ts"]
}
